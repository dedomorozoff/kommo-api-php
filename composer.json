{
    "name": "andrey-tech/amocrm-api-php",
    "description": "Simple wrapper PHP7+ for API kommo/amoCRM with oAuth 2.0 ",
    "keywords": [ "kommo","amocrm", "crm", "api", "api-wrapper", "oauth2", "AJAX", "throttling", "logging" ],
    "homepage": "https://github.com/dedomorozoff/kommo-api-php",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "andrey-tech",
            "homepage": "https://github.com/andrey-tech/",
            "role": "Developer"
        },
        {
            "name": "dedomorozoff",
            "homepage": "https://github.com/dedomorozoff/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Kommo\\": "src/Kommo"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
